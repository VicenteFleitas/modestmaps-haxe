package com.modestmaps.extras.ui;import openfl.display.MovieClip;import openfl.events.MouseEvent;    /** * Sample Marker * @author David Knape */class SampleMarker extends MovieClip{	   	private var _title : String;	public function SampleMarker() {		stop();       					buttonMode = true;		mouseChildren = false;		tabEnabled = false;        			cacheAsBitmap = true;				addEventListener( MouseEvent.ROLL_OVER, bringToFront, true );	}				public function get title () : String {		return _title;	}		public function set title (s:String) : void {		_title = s;	}				private function bringToFront(e:MouseEvent) : void {		parent.swapChildrenAt(parent.getChildIndex(this), parent.numChildren - 1);	}		override public function toString() : String {		return '[SampleMarker] ' + title;	}}